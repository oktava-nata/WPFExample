<Window x:Class="Shared.User.Forms.UsersForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:strRes="clr-namespace:Shared.Properties"
        xmlns:userPanels="clr-namespace:Shared.User.Forms"
        Title="{x:Static strRes:Resources.txtUserManagement}" Height="500" Width="1000" MinHeight="400" MinWidth="600" 
        WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" Style="{StaticResource StandartWindowStyle}">
    <Grid Style="{StaticResource FormGrid}">
        <userPanels:UserListPanel x:Name="UserListPanel"></userPanels:UserListPanel>
    </Grid>
</Window>
